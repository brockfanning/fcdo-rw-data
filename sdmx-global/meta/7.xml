        <gen:ReportedAttribute id="STAT_CONC_DEF">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Definition</h2><p>It is the number of households who use electricity as their main source for lighting to the total number of households expressed as a percentage.</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="UNIT_MEASURE">
          
          <com:Text xml:lang="en"><![CDATA[<p>Percentage (%)</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="FREQ_COLL">
          
          <com:Text xml:lang="en"><![CDATA[<p>3 years</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="REC_USE_LIM">
          
          <com:Text xml:lang="en"><![CDATA[<p>EICV considers <strong>only grid connected households </strong>using electricity<strong> </strong>as their main source of home lighting (National connection grid and local mini grid are combined together). Off grid sources like solar panel, generator and rechargeable batteries are reported separately.</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_COMP">
          
          <com:Text xml:lang="en"><![CDATA[<p>The indicator is computed as; </p><p>Pe / Pt</p><p>Where Pe denotes the number of households who declared using electricity as their main source for lighting and Pt denotes the total number of urban households.</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
