        <gen:ReportedAttribute id="SDG_INDICATOR">
          
          <com:Text xml:lang="en"><![CDATA[<p>16.1.4</p><p>Proportion of population that feel safe walking alone around the area they live</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_CUSTODIAN_AGENCIES">
          
          <com:Text xml:lang="en"><![CDATA[<p>United Nations Office On Drugs and Crime (UNODC)</p>]]></com:Text>
          
