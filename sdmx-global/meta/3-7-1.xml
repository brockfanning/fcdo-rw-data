        <gen:ReportedAttribute id="SDG_TARGET">
          
          <com:Text xml:lang="en"><![CDATA[<p>3.7</p><p>By 2030, ensure universal access to sexual and reproductive health-care services, including for family planning, information and education, and the integration of reproductive health into national strategies and programmes</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_INDICATOR">
          
          <com:Text xml:lang="en"><![CDATA[<p>3.7.1</p><p>Proportion of women of reproductive age (aged 15-49 years) who have their need for family planning satisfied with modern methods</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_CUSTODIAN_AGENCIES">
          
          <com:Text xml:lang="en"><![CDATA[<p>Department of Economic and Social Affairs (DESA), Population Division</p><p>United Nations Population Fund (UNFPA)</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
