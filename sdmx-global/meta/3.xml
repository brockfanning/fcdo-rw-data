    <gen:Report id="SDG_META_RPT">
      <gen:Target id="KEY_TARGET">
        <gen:ReferenceValue id="DIMENSION_DESCRIPTOR_VALUES_TARGET">
          <gen:DataKey>
            <com:KeyValue id="SERIES">
              <com:Value>SH_HIV_INCD</com:Value>
            </com:KeyValue>
            <com:KeyValue id="REPORTING_TYPE">
              <com:Value>N</com:Value>
            </com:KeyValue>
            <com:KeyValue id="REF_AREA">
              <com:Value>RW</com:Value>
            </com:KeyValue>
          </gen:DataKey>
        </gen:ReferenceValue>
        <gen:ReferenceValue id="Dataflow">
          <gen:ObjectReference>
            <URN>urn:sdmx:org.sdmx.infomodel.datastructure.Dataflow=IAEG-SDGs:DF_SDG_GLC(1.0)</URN>
          </gen:ObjectReference>
        </gen:ReferenceValue>
      </gen:Target>
      <gen:AttributeSet>
        
        <gen:ReportedAttribute id="SDG_TARGET">
          
          <com:Text xml:lang="en"><![CDATA[<p>3.3</p><p>By 2030, end the epidemics of AIDS, tuberculosis, malaria and neglected tropical diseases and combat hepatitis, water-borne diseases and other communicable diseases</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_INDICATOR">
          
          <com:Text xml:lang="en"><![CDATA[<p>3.3.1</p><p>Number of new HIV infections per 1,000 uninfected population, by sex, age and key populations</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_CUSTODIAN_AGENCIES">
          
          <com:Text xml:lang="en"><![CDATA[<p>The Joint United Nations Programme on HIV/AIDS (UNAIDS)</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="CONTACT_ORGANISATION">
          
          <com:Text xml:lang="en"><![CDATA[<p>Ministry of Health (MoH)</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="STAT_CONC_DEF">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Indicator available</h2><p>HIV Incidence </p><h2>Definition</h2><p>The incidence rate is the number of new cases per population at risk in a given time period.</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="UNIT_MEASURE">
          
          <com:Text xml:lang="en"><![CDATA[<p>Per 1,000 uninfected population</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="FREQ_COLL">
          
          <com:Text xml:lang="en"><![CDATA[<p>3-5 years</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_COMP">
          
          <com:Text xml:lang="en"><![CDATA[<p>It is calculated as </p><p>Number of new HIV infections / Uninfected persons</p><p> x 1,000</p><p>Uninfected population (which is the total population minus people living with HIV)</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DOC_METHOD">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Link to global metadata</h2><p><a href="https://unstats.un.org/sdgs/metadata/files/Metadata-03-03-01.pdf">https://unstats.un.org/sdgs/metadata/files/Metadata-03-03-01.pdf</a></p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COVERAGE">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Geographical coverage</h2><p>National</p><h2>Disaggregation</h2><ul><li>Residence</li><li>Age group</li><li>Sex </li><li>Marital Status</li><li>Risk Factors</li></ul>]]></com:Text>
          
        </gen:ReportedAttribute>
        
