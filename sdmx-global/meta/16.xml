      </gen:Target>
      <gen:AttributeSet>
        
        <gen:ReportedAttribute id="SDG_TARGET">
          
          <com:Text xml:lang="en"><![CDATA[<p>19.9</p><p>By 2030, provide legal identity for all, including birth registration</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_INDICATOR">
          
          <com:Text xml:lang="en"><![CDATA[<p>16.9.1</p><p>Proportion of children under 5 years of age whose births have been registered with a civil authority, by age</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_CUSTODIAN_AGENCIES">
          
          <com:Text xml:lang="en"><![CDATA[<p>United Nations Statistics Division (UNSD), United Nations International Children&#39;s Emergency Fund (UNICEF)</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="CONTACT_ORGANISATION">
          
          <com:Text xml:lang="en"><![CDATA[<p>National Institute of Statistics of Rwanda (NISR)</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="CONTACT_EMAIL">
          
          <com:Text xml:lang="en"><![CDATA[<p><a href="mailto:info@statistics.gov.rw">info@statistics.gov.rw</a></p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="STAT_CONC_DEF">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Indicator available</h2><p>Percentage of de jure children under age 5 whose births are registered with the civil authorities </p><h2>Definition</h2><p>Proportion of children under 5 years of age whose births have been registered with a civil authority.</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="UNIT_MEASURE">
          
          <com:Text xml:lang="en"><![CDATA[<p>Percent</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="FREQ_COLL">
          
          <com:Text xml:lang="en"><![CDATA[<p>3-5 Years</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_COMP">
          
          <com:Text xml:lang="en"><![CDATA[<p>Number of children under age of five whose births are reported as being registered with the relevant national civil authorities divided by the total number of children under the age of five in the population multiplied by 100</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DOC_METHOD">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Link to global metadata</h2><p><a href="https://unstats.un.org/sdgs/metadata/files/Metadata-16-09-01.pdf" target="_blank">United Nations Sustainable Development Goals Metadata (pdf 1361kB)</a></p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COVERAGE">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Earliest available data</h2><p>2005</p><h2>Geographical coverage</h2><p>National</p><h2>Disaggregation</h2><ul><li>Geographical: National, Province, District Residence (Urban &amp; Rural)</li><li>Sex</li><li>Age in months</li><li>Wealth Quintile</li></ul>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="OTHER_DOC">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Link to data source</h2><p><a href="http://www.statistics.gov.rw/datasource/demographic-and-health-survey-dhs">http://www.statistics.gov.rw/datasource/demographic-and-health-survey-dhs</a></p><h2>UN designated tier</h2><p>1</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
      </gen:AttributeSet>
    </gen:Report>
  </mes:MetadataSet>
  
