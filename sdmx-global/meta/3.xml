        <gen:ReportedAttribute id="SDG_TARGET">
          
          <com:Text xml:lang="en"><![CDATA[<p>3.a</p><p>Strengthen the implementation of the World Health Organization Framework Convention on Tobacco Control in all countries, as appropriate</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_INDICATOR">
          
          <com:Text xml:lang="en"><![CDATA[<p>3.a.1</p><p>Age-standardized prevalence of current tobacco use among persons aged 15 years and older</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_CUSTODIAN_AGENCIES">
          
          <com:Text xml:lang="en"><![CDATA[<p>World Health Organization (WHO) Framework Convention on Tobacco Control (FCTC)</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="CONTACT_ORGANISATION">
          
          <com:Text xml:lang="en"><![CDATA[<p>National Institute of Statistics of Rwanda</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="CONTACT_EMAIL">
          
          <com:Text xml:lang="en"><![CDATA[<p><a href="mailto:info@statistics.gov.rw">info@statistics.gov.rw</a></p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="STAT_CONC_DEF">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Indicator available</h2><p>Percentage of men/women age 15-49 who smoke cigarettes or a pipe or use other tobacco products</p><h2>Definition</h2><p>The indicator is defined as the percentage of the population aged 15 years and over who currently use any tobacco product (smoked and/or smokeless tobacco) on a daily or non-daily basis.</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="UNIT_MEASURE">
          
          <com:Text xml:lang="en"><![CDATA[<p>Percent (%)</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="FREQ_COLL">
          
          <com:Text xml:lang="en"><![CDATA[<p>3-5 Years</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_COMP">
          
          <com:Text xml:lang="en"><![CDATA[<p>It is calculated as; </p><p>Number of Current tobacco users age 15-19 years / Total population</p><p>“Current users” include both daily and non-daily users of smoked or smokeless tobacco</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DOC_METHOD">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Link to global metadata</h2><p><a href="https://unstats.un.org/sdgs/metadata/files/Metadata-03-0a-01.pdf">https://unstats.un.org/sdgs/metadata/files/Metadata-03-0a-01.pdf</a></p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COVERAGE">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Earliest available data</h2><p>1992</p><h2>Latest available data</h2><p>2014-15</p><h2>Geographical coverage</h2><p>National</p><h2>Disaggregation</h2><ul><li>Geographical: National, Province, District, Residence (Urban &amp; Rural)</li><li>Sex</li><li>Education</li><li>Maternity status</li><li>wealth quintiles</li></ul>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="OTHER_DOC">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Link to data source</h2><p><a href="http://www.statistics.gov.rw/datasource/demographic-and-health-survey-dhs">http://www.statistics.gov.rw/datasource/demographic-and-health-survey-dhs</a></p><h2>UN designated tier</h2><p>1</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
      </gen:AttributeSet>
    </gen:Report>
