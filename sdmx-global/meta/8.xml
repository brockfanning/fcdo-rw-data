        <gen:ReportedAttribute id="SDG_INDICATOR">
          
          <com:Text xml:lang="en"><![CDATA[<p>8.10.2</p><p>Proportion of adults (15 years and older) with an account at a bank or other financial institution or with a mobile-money-service provider</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_CUSTODIAN_AGENCIES">
          
