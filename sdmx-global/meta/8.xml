        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="FREQ_COLL">
          
          <com:Text xml:lang="en"><![CDATA[<p>Annual</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="REC_USE_LIM">
          
          <com:Text xml:lang="en"><![CDATA[<p>In Rwanda, youth is defined as all persons between the ages is 16-24 years</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_COMP">
          
          <com:Text xml:lang="en"><![CDATA[<p>It can be calculated as;</p><p>Number of Adult having an account at bank or other type of institutions divided by the total adult population multiplied by 100.</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DOC_METHOD">
          
