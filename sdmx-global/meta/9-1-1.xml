        <gen:ReportedAttribute id="SDG_TARGET">
          
          <com:Text xml:lang="en"><![CDATA[<p>9.1</p><p>Develop quality, reliable, sustainable and resilient infrastructure, including regional and trans-border infrastructure, to support economic development and human well-being, with a focus on affordable and equitable access for all</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_INDICATOR">
          
          <com:Text xml:lang="en"><![CDATA[<p>9.1.1</p><p>Proportion of the rural population who live within 2 km of an all-season road</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
