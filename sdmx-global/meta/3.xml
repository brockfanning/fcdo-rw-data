        <gen:ReportedAttribute id="SDG_TARGET">
          
          <com:Text xml:lang="en"><![CDATA[<p>3.1</p><p>By 2030, reduce the global maternal mortality ratio to less than 70 per 100,000 live births</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_INDICATOR">
          
          <com:Text xml:lang="en"><![CDATA[<p>3.1.2</p><p>Proportion of births attended by skilled health personnel</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_CUSTODIAN_AGENCIES">
          
          <com:Text xml:lang="en"><![CDATA[<p>UNICEF</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="CONTACT_ORGANISATION">
          
          <com:Text xml:lang="en"><![CDATA[<p>National Institute of Statistics of Rwanda</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="CONTACT_EMAIL">
          
          <com:Text xml:lang="en"><![CDATA[<p><a href="mailto:info@statistics.gov.rw">info@statistics.gov.rw</a></p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="STAT_CONC_DEF">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Indicator available</h2><p>Percentage distribution of live birth in the five years preceding the survey by person providing assistance during delivery, according to background characteristics</p><h2>Definition</h2><p>Is the proportion of total live births in a period that are attended by a skilled birth attendant trained in providing lifesaving obstetric care.</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="UNIT_MEASURE">
          
          <com:Text xml:lang="en"><![CDATA[<p>Percentage (%)</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="FREQ_COLL">
          
          <com:Text xml:lang="en"><![CDATA[<p>3-5 Years</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_COMP">
          
          <com:Text xml:lang="en"><![CDATA[<p>Calculated as the number of births attended by skilled health personnel (doctors, nurses or midwives) divided by the total number of births in the same period and multiplied by 100.</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DOC_METHOD">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Link to global metadata</h2><p><a href="https://unstats.un.org/sdgs/metadata/files/Metadata-03-01-02.pdf" target="_blank">United Nations Sustainable Development Goals Metadata (PDF 374 KB)</a></p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COVERAGE">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Earliest available data</h2><p>1992</p><h2>Latest available data</h2><p>2014-15</p><h2>Geographical coverage</h2><p>National </p><h2>Disaggregation</h2><ul><li>National, Province, Districts, Residence (Urban/ Rural)</li><li>Mother’s age at birth, Birth order, Mother’s education level,</li><li>Place of Delivery (Health facility, Elsewhere), Wealth quintiles</li></ul>]]></com:Text>
          
