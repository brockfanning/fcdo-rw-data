        <gen:ReportedAttribute id="CONTACT_EMAIL">
          
          <com:Text xml:lang="en"><![CDATA[<p><a href="mailto:info@statistics.gov.rw">info@statistics.gov.rw</a></p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="STAT_CONC_DEF">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Indicator available</h2><p>Percentage of HHs that are within 2km to an all-weather road by region (Urban and Rural)</p><h2>Definition</h2><p>This indicator refers to the proportion of households having access to an all-season road within 2km to the total number of households using the facility.</p><p>According to the context of Rwanda, an all-season road is defined as a road that is open to traffic regardless of weather, season and construction materials 2 kilometres</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="UNIT_MEASURE">
          
          <com:Text xml:lang="en"><![CDATA[<p>Percentage </p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="FREQ_COLL">
          
          <com:Text xml:lang="en"><![CDATA[<p>3 years</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_COMP">
          
          <com:Text xml:lang="en"><![CDATA[<p>The indicator is calculated as: </p><p>n / N x 100</p><p>Where </p><p>n: denotes the total number of households having access to an all-season road within 2km.</p><p>N: denotes the total number of households using all-season road.</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DOC_METHOD">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Link to global metadata</h2><p><a href="https://unstats.un.org/sdgs/tierIII-indicators/" target="_blank">United Nations Sustainable Development Goals Metadata (PDF 4.0 MB)</a></p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
